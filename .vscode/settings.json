{
    "cSpell.enableFiletypes": [
        "!json"
    ],
    "cSpell.words": [
        "buildx",
        "bzip",
        "CRACO",
        "freetype",
        "gtid",
        "libiconv",
        "libpng",
        "libsodium",
        "libxml",
        "libzip",
        "Listr",
        "Magento",
        "mysqldump",
        "opensearch",
        "overriden",
        "portscanner",
        "REBEM",
        "scandipwa",
        "Scandiweb",
        "searchengine",
        "tablespaces",
        "xdebug"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "debug.javascript.autoAttachFilter": "onlyWithFlag",
    "debug.javascript.terminalOptions": {
        "skipFiles": [
          "<node_internals>/**"
        ]
    },
    "githubPullRequests.queries": [
        {
            "label": "Opened PRs",
            "query": "is:pr is:open sort:created-desc"
        },
        {
            "label": "Waiting For My Review",
            "query": "is:pr is:open review-requested:${user}"
        },
        {
            "label": "Assigned To Me",
            "query": "is:pr is:open assignee:${user}"
        },
        {
            "label": "Created By Me",
            "query": "is:pr is:open author:${user}"
        }
    ],
    "githubPullRequests.remotes": [
        "origin"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules": true,
        "**/node_modules/**": true,
        "**/node_modules/*/**": true,
        "**/tmp/**": true,
        "**/dist/**": true,
        "**/vendor": true,
        "**/vendor/**": true,
        "**/vendor/*/**": true
    }
}